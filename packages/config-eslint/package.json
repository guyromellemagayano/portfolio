{
  "name": "@guyromellemagayano/config-eslint",
  "version": "1.0.0",
  "private": true,
  "description": "Shared ESLint config presets",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./react": {
      "types": "./src/presets/react.d.ts",
      "default": "./src/presets/react.js"
    },
    "./next": {
      "types": "./src/presets/next.d.ts",
      "default": "./src/presets/next.js"
    }
  },
  "scripts": {
    "clean": "rimraf node_modules .turbo dist",
    "format": "prettier --write \"src/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "format:check": "prettier --check \"src/**/*.{js,cjs,mjs,ts,cts,mts,md,mdx}\"",
    "lint": "eslint --ext .js,.cjs,.mjs,.ts,.cts,.mts src/",
    "lint:fix": "eslint --fix --ext .js,.cjs,.mjs,.ts,.cts,.mts src/"
  },
  "dependencies": {
    "@eslint/js": "catalog:",
    "@next/eslint-plugin-next": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "@typescript-eslint/type-utils": "catalog:",
    "@typescript-eslint/utils": "catalog:",
    "@vitest/eslint-plugin": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-import-resolver-typescript": "catalog:",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-jest-dom": "catalog:",
    "eslint-plugin-only-warn": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "eslint-plugin-react": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "eslint-plugin-react-refresh": "catalog:",
    "eslint-plugin-simple-import-sort": "catalog:",
    "eslint-plugin-testing-library": "catalog:",
    "eslint-plugin-turbo": "catalog:",
    "eslint-plugin-unused-imports": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "devDependencies": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "globals": "catalog:",
    "postcss": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {},
  "optionalDependencies": {}
}
