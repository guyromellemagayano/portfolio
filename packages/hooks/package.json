{
  "name": "@guyromellemagayano/hooks",
  "version": "1.0.0",
  "description": "A collection of reusable React hooks that provide common functionality across components.",
  "keywords": [
    "react",
    "hooks",
    "typescript"
  ],
  "homepage": "https://github.com/guyromellemagayano/portfolio/tree/main/packages/hooks",
  "bugs": {
    "url": "https://github.com/guyromellemagayano/portfolio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guyromellemagayano/portfolio.git",
    "directory": "packages/hooks"
  },
  "license": "MIT",
  "author": "Guy Romelle Magayano <aspiredtechie2010@gmail.com> (https://www.guyromellemagayano.dev)",
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "bunchee",
    "check-types": "tsc --noEmit",
    "clean": "rimraf node_modules .turbo dist coverage",
    "dev": "bunchee --watch",
    "format": "prettier --write \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx,jsx,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx,jsx,tsx}\"",
    "lint": "eslint \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx,jsx,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx,jsx,tsx}\"",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:run": "vitest run",
    "test:run:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@guyromellemagayano/logger": "workspace:*"
  },
  "devDependencies": {
    "@guyromellemagayano/config-eslint": "workspace:*",
    "@guyromellemagayano/config-typescript": "workspace:*",
    "@guyromellemagayano/vitest-presets": "workspace:*",
    "@testing-library/react": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "bunchee": "catalog:",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "postcss": "catalog:",
    "prettier": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "optionalDependencies": {},
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  }
}
