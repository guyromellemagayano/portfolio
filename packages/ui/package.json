{
  "name": "@guyromellemagayano/ui",
  "version": "1.0.0",
  "description": "Enhanced UI components library with analytics, accessibility, and performance optimizations",
  "keywords": [
    "react",
    "ui-components",
    "typescript",
    "next.js",
    "accessibility",
    "analytics"
  ],
  "homepage": "https://github.com/guyromellemagayano/portfolio/tree/main/packages/ui",
  "bugs": {
    "url": "https://github.com/guyromellemagayano/portfolio/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guyromellemagayano/portfolio.git",
    "directory": "packages/ui"
  },
  "license": "MIT",
  "author": "Guy Romelle Magayano <aspiredtechie2010@gmail.com> (https://www.guyromellemagayano.dev)",
  "sideEffects": true,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "main": "./dist/es/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "bunchee",
    "build-storybook": "storybook build",
    "check-types": "tsc --noEmit",
    "clean": "rimraf node_modules .turbo dist coverage",
    "dev": "bunchee --watch",
    "format": "prettier --write \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx,jsx,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx,jsx,tsx}\"",
    "lint": "eslint \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx,jsx,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx,jsx,tsx}\"",
    "storybook": "storybook dev -p 6006",
    "test": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:run": "vitest run",
    "test:run:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@guyromellemagayano/logger": "workspace:*"
  },
  "devDependencies": {
    "@guyromellemagayano/config-eslint": "workspace:*",
    "@guyromellemagayano/config-typescript": "workspace:*",
    "@guyromellemagayano/utils": "workspace:*",
    "@guyromellemagayano/vitest-presets": "workspace:*",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/user-event": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "bunchee": "catalog:",
    "eslint": "catalog:",
    "nodemon": "catalog:",
    "prettier": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  }
}
