{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build --webpack",
    "check-types": "next typegen && tsc --noEmit",
    "clean": "rimraf node_modules .turbo .next coverage",
    "dev": "next dev --hostname 0.0.0.0 --port 3000 --turbopack",
    "format": "prettier --write \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx,jsx,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx,jsx,tsx}\"",
    "lint": "eslint \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx,jsx,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx,jsx,tsx}\" --max-warnings 0",
    "lint:styles": "stylelint \"src/**/*.{css,scss}\" --allow-empty-input",
    "lint:styles:fix": "stylelint \"src/**/*.{css,scss}\" --fix --allow-empty-input",
    "sanity": "sanity",
    "sanity:deploy": "sanity deploy",
    "start": "next start",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:coverage:ui": "vitest --coverage --ui",
    "test:run": "vitest run",
    "test:run:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
