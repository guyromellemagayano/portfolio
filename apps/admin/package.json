{
  "name": "admin",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check-types": "tsc --noEmit",
    "clean": "rimraf node_modules .turbo dist coverage",
    "dev": "vite --host 0.0.0.0 --port 3001 --clearScreen false",
    "format": "prettier --write \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx}\"",
    "lint": "eslint src/ --max-warnings 0",
    "lint:fix": "eslint --fix src/ --max-warnings 0",
    "test": "echo 'No tests configured for admin app'",
    "test:coverage": "echo 'No tests configured for admin app'",
    "test:run": "echo 'No tests configured for admin app'",
    "test:run:coverage": "echo 'No tests configured for admin app'",
    "test:ui": "echo 'No tests configured for admin app'"
  },
  "dependencies": {
    "@guyromellemagayano/ui": "workspace:*"
  },
  "devDependencies": {
    "@packages/eslint-config": "workspace:*",
    "@packages/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-router": "catalog:",
    "rimraf": "catalog:",
    "sharp": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-devtools-json": "catalog:"
  }
}
