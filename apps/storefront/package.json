{
  "name": "storefront",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "next build",
    "check-types": "tsc --noEmit",
    "clean": "rimraf node_modules .turbo .next coverage",
    "dev": "next dev --hostname 0.0.0.0 --port 3002 --turbopack",
    "format": "prettier --write \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,cjs,mjs,ts,cts,mts,tsx}\"",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "start": "next start",
    "test": "echo 'No tests configured for storefront app'",
    "test:coverage": "echo 'No tests configured for storefront app'",
    "test:run": "echo 'No tests configured for storefront app'",
    "test:run:coverage": "echo 'No tests configured for storefront app'",
    "test:ui": "echo 'No tests configured for storefront app'"
  },
  "dependencies": {
    "@guyromellemagayano/components": "workspace:*",
    "@guyromellemagayano/logger": "workspace:*",
    "@guyromellemagayano/ui": "workspace:*",
    "next": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@guyromellemagayano/config-eslint": "workspace:*",
    "@guyromellemagayano/config-typescript": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "rimraf": "catalog:",
    "sharp": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {},
  "optionalDependencies": {}
}