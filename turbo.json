{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "concurrency": "12",
  "remoteCache": {
    "enabled": true,
    "signature": true
  },
  "globalDependencies": [
    ".env*",
    "package.json",
    "pnpm-lock.yaml",
    "pnpm-workspace.yaml",
    "tsconfig.json"
  ],
  "globalEnv": [
    "ANALYZE",
    "AWS_ACCESS_KEY_ID",
    "AWS_REGION",
    "AWS_SECRET_ACCESS_KEY",
    "AZURE_INSTRUMENTATION_KEY",
    "BUNDLE_ANALYZE",
    "CI",
    "COOKIE_HTTPONLY",
    "COOKIE_SAMESITE",
    "COOKIE_SECURE",
    "COREPACK_HOME",
    "CORS_ORIGINS",
    "DATABASE_URL",
    "DATADOG_API_KEY",
    "DEBUG",
    "E2E_BASE_URL",
    "ELASTICSEARCH_API_KEY",
    "ELASTICSEARCH_URL",
    "ENABLE_EXPERIMENTAL_COREPACK",
    "ENVIRONMENT",
    "ESLINT_USE_FLAT_CONFIG",
    "GIT_HASH",
    "GOOGLE_ADSENSE_CLIENT_ID",
    "GOOGLE_ANALYTICS_MEASUREMENT_ID",
    "GOOGLE_CLOUD_KEY_FILE",
    "GOOGLE_CLOUD_PROJECT_ID",
    "GOOGLE_TAG_MANAGER_CONTAINER_ID",
    "JWT_ACCESS_TOKEN_EXPIRE_MINUTES",
    "JWT_ALGORITHM",
    "JWT_REFRESH_TOKEN_EXPIRE_DAYS",
    "JWT_SECRET_KEY",
    "LOCAL_ELASTICSEARCH",
    "LOGROCKET_APP_ID",
    "LOGROCKET_TOKEN",
    "NEWRELIC_LICENSE_KEY",
    "NEXT_PUBLIC_API_URL",
    "NEXT_PUBLIC_GRAPHQL_URL",
    "NEXT_PUBLIC_PORTAL_SITE_GRAPHQL_API_PATHNAME",
    "NEXT_PUBLIC_PORTAL_SITE_URL",
    "NEXT_PUBLIC_SANITY_API_VERSION",
    "NEXT_PUBLIC_SANITY_DATASET",
    "NEXT_PUBLIC_SANITY_PROJECT_ID",
    "NEXT_PUBLIC_SENTRY_DSN",
    "NODE_ENV",
    "NPM_TOKEN",
    "PAPERTRAIL_HOST",
    "PAPERTRAIL_PORT",
    "PORT",
    "POSTGRES_PORT",
    "REDIS_PORT",
    "REDIS_URL",
    "SANITY_API_READ_TOKEN",
    "SANITY_API_VERSION",
    "SANITY_DATASET",
    "SANITY_PROJECT_ID",
    "SANITY_USE_CDN",
    "SENTRY_AUTH_TOKEN",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SPLUNK_HEC_TOKEN",
    "SPLUNK_HOST",
    "SPLUNK_PORT",
    "TURBO_REMOTE_CACHE_SIGNATURE_KEY",
    "TURBO_TEAM",
    "TURBO_TOKEN",
    "VERCEL_ENV",
    "VERCEL_PROJECT_PRODUCTION_URL",
    "VERCEL_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        ".env*",
        "$TURBO_DEFAULT$",
        "next.config.*",
        "package.json",
        "pnpm-lock.yaml",
        "postcss.config.*",
        "tailwind.config.*",
        "tsconfig*.json",
        "vite.config.*"
      ],
      "outputs": [
        "!.next/cache/**",
        "!.next/standalone/**",
        ".next/**",
        "dist/**"
      ]
    },
    "codegen": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "codegen.*",
        "codegen.yml",
        "schema.*",
        "*.graphql",
        "*.gql",
        "package.json"
      ],
      "outputs": [
        "**/generated.ts",
        "**/__generated__/**",
        "src/gql/**",
        "src/__generated__/**"
      ],
      "cache": true
    },
    "test": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "vitest.config.*",
        "vitest.workspace.*",
        "src/test-setup.*",
        "**/*.test.{ts,tsx,js,jsx}",
        "**/*.spec.{ts,tsx,js,jsx}",
        "**/__tests__/**"
      ],
      "outputs": [],
      "cache": false
    },
    "test:ui": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "vitest.config.*",
        "vitest.workspace.*",
        "src/test-setup.*",
        "**/*.test.{ts,tsx,js,jsx}",
        "**/*.spec.{ts,tsx,js,jsx}",
        "**/__tests__/**"
      ],
      "outputs": [],
      "cache": false,
      "persistent": true
    },
    "test:run": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "vitest.config.*",
        "vitest.workspace.*",
        "src/test-setup.*",
        "**/*.test.{ts,tsx,js,jsx}",
        "**/*.spec.{ts,tsx,js,jsx}",
        "**/__tests__/**"
      ],
      "outputs": [],
      "cache": true
    },
    "test:coverage": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "vitest.config.*",
        "vitest.workspace.*",
        "src/test-setup.*",
        "**/*.test.{ts,tsx,js,jsx}",
        "**/*.spec.{ts,tsx,js,jsx}",
        "**/__tests__/**"
      ],
      "outputs": [
        "coverage/**"
      ],
      "cache": false
    },
    "test:coverage:ui": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "vitest.config.*",
        "vitest.workspace.*",
        "src/test-setup.*",
        "**/*.test.{ts,tsx,js,jsx}",
        "**/*.spec.{ts,tsx,js,jsx}",
        "**/__tests__/**"
      ],
      "outputs": [
        "coverage/**"
      ],
      "cache": false,
      "persistent": true
    },
    "test:run:coverage": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "vitest.config.*",
        "vitest.workspace.*",
        "src/test-setup.*",
        "**/*.test.{ts,tsx,js,jsx}",
        "**/*.spec.{ts,tsx,js,jsx}",
        "**/__tests__/**"
      ],
      "outputs": [
        "coverage/**"
      ],
      "cache": true
    },
    "test:e2e": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "playwright.config.*",
        "tests/**/*.e2e.ts",
        "tests/**/*.setup.ts"
      ],
      "outputs": [
        "playwright-report/**",
        "test-results/**"
      ],
      "cache": false
    },
    "test:e2e:smoke": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "playwright.config.*",
        "tests/**/*.e2e.ts",
        "tests/**/*.setup.ts"
      ],
      "outputs": [
        "playwright-report/**",
        "test-results/**"
      ],
      "cache": false
    },
    "test:e2e:ui": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "playwright.config.*",
        "tests/**/*.e2e.ts",
        "tests/**/*.setup.ts"
      ],
      "outputs": [],
      "cache": false,
      "persistent": true
    },
    "test:e2e:headed": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "playwright.config.*",
        "tests/**/*.e2e.ts",
        "tests/**/*.setup.ts"
      ],
      "outputs": [
        "playwright-report/**",
        "test-results/**"
      ],
      "cache": false,
      "persistent": true
    },
    "test:e2e:report": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "playwright-report/**"
      ],
      "outputs": [],
      "cache": false,
      "persistent": true
    },
    "format": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "prettier.config.*",
        ".prettierrc*",
        ".prettierignore"
      ],
      "outputs": [],
      "cache": true
    },
    "format:check": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "prettier.config.*",
        ".prettierrc*",
        ".prettierignore"
      ],
      "outputs": [],
      "cache": true
    },
    "lint": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "eslint.config.*",
        ".eslintrc*",
        ".eslintignore"
      ],
      "outputs": [],
      "cache": true
    },
    "lint:fix": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "eslint.config.*",
        ".eslintrc*",
        ".eslintignore"
      ],
      "outputs": [],
      "cache": false
    },
    "check-types": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "tsconfig.json",
        "tsconfig.*.json"
      ],
      "outputs": [],
      "cache": true
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "persistent": true,
      "cache": false
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ],
      "outputs": [],
      "cache": false
    },
    "lint:styles": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "stylelint.config.*",
        ".stylelintrc*",
        "**/*.{css,scss}"
      ],
      "outputs": [],
      "cache": true
    },
    "lint:styles:fix": {
      "dependsOn": [],
      "inputs": [
        "$TURBO_DEFAULT$",
        "stylelint.config.*",
        ".stylelintrc*",
        "**/*.{css,scss}"
      ],
      "outputs": [],
      "cache": false
    }
  }
}
